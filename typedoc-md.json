{
  "$schema": "https://typedoc-plugin-markdown.org/schema.json",
  "name": "marker.js 3 Documentation",
  "plugin": ["typedoc-plugin-markdown"],
  "projectDocuments": ["docs-src/guides.md", "docs-src/guides/**/*.md"],
  "entryPoints": [
    "./src/core.ts",
    "./src/editor.ts",
    "./src/viewer.ts",
    "./src/Renderer.ts"
  ],
  "readme": "docs-src/README.md",
  "sortEntryPoints": false,
  "categoryOrder": ["Documentation", "API Reference"],
  "groupOrder": ["Components", "Markers", "Editors", "*"],
  "sort": ["alphabetical"],
  "navigation": {
    "includeCategories": true
  },
  "navigationLinks": {
    "marker.js website": "https://markerjs.com"
  },
  "outputFileStrategy": "modules",

  "out": "./docs-md"
}
