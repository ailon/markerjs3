{
  "compilerOptions": {
    "strict": true,
    "lib": ["es2015", "dom"],
    "target": "es6",
    "forceConsistentCasingInFileNames": true,
    "preserveSymlinks": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules"]
}
