{
  "$schema": "https://typedoc.org/schema.json",
  "name": "marker.js 3 Documentation",
  "projectDocuments": ["docs-src/guides.md"],
  "entryPoints": [
    "./src/core.ts",
    "./src/editor.ts",
    "./src/viewer.ts",
    "./src/Renderer.ts"
  ],
  "readme": "docs-src/README.md",
  "sortEntryPoints": false,
  "categoryOrder": ["Documentation", "API Reference"],
  "groupOrder": ["Components", "Markers", "Editors", "*"],
  "sort": ["alphabetical"],
  "navigation": {
    "includeCategories": true
  },
  "navigationLinks": {
    "marker.js website": "https://markerjs.com"
  },
  "customCss": "docs-src/mjs-custom.css",
  "lightHighlightTheme": "github-light",
  "plugin": ["@8hobbies/typedoc-plugin-plausible"],
  "plausibleSiteDomain": "markerjs.com",

  "out": "./docs"
}
